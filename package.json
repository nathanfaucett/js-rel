{
  "name": "rel",
  "version": "0.0.1",
  "description": "relational algebra for the browser and node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanfaucett/rel.git"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-node",
    "test-node": "node ./test | ./node_modules/.bin/tap-spec",
